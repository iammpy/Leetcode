#include <iostream>
#include <cmath>
#include <vector>
#include <queue>
using namespace std;


int main() {
    int n;
    cin>>n;

    for(int i=0;i<n;++i){
        int ai,bi,pi;
        cin>>ai;
        cin>>bi;
        cin>>pi;
        int temp=ai;
        int num=1;
        while(bi!=0){
            int k=bi%2;
            bi=bi/2;
            if(k==1) num*=temp*k;
            temp*=temp;
        }

        cout<<num%pi<<endl;

    }



    return 0;

}
