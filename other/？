#include <iostream>
#include <cmath>
#include <vector>
#include <queue>
using namespace std;


int main() {
    int n;
    cin>>n;
    vector<int> res;
    for(int i=0;i<n;++i){
        int ai,bi,pi;
        cin>>ai;
        cin>>bi;
        cin>>pi;
        int temp=ai%pi;
        int num=1;
        while(bi!=0){
            int k=bi%2;
            bi=bi/2;
            if(k==1) num=num*temp*k%pi;
            temp*=temp%pi;
        }

        res.push_back(num);

    }
    for(int i=0;i<n;i++){
        cout<<res[i]<<endl;
    }



    return 0;

}
